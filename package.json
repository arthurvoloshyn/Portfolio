  "private": true,
  "description": "My own portfolio build on React",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArturW1998/Portfolio"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux"
  ],
  "author": "Artur Voloshyn (https://github.com/arturw1998)",
  "license": "MIT",
  "dependencies": {
    "@fullpage/react-fullpage": "0.1.17",
    "classnames": "^2.2.6",
    "leaflet": "^1.4.0",
    "modernizr": "^3.6.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-leaflet": "^1.9.1",
    "react-parallax-mousemove": "^1.1.0",
    "react-redux": "^7.1.0",
    "react-rotating-text": "^1.2.1",
    "react-router-dom": "^5.1.2",
    "react-s-alert": "^1.4.1",
    "react-scripts": "5.0.1",
    "redux": "^4.0.1",
    "simplex-noise": "^2.4.0"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "stylelint": "^12.0.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:update": "npm test -- -u",
    "test:watch": "npm test -- --watchAll --verbose",
    "test:coverage": "npm test -- --bail --coverage --watchAll",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint:styles": "stylelint --syntax scss src/**/*.scss",
    "lint": "eslint --ext .js,.jsx src",
    "lint:fix": "npm run lint -- --fix",
    "lint:styles:fix": "npm run lint:styles -- --fix",
    "format": "prettier \"src/**/*.{js,jsx,yml,yaml,ts,tsx,md,graphql,mdx}\" --write",
    "format:styles": "prettier \"src/**/*.scss\" --parser scss --write",
    "precommit": "lint-staged",
    "validate": "npm run lint && npm run lint:styles && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
